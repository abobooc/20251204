            width: 100px;  /* &amp;#x5BEC;&amp;#x5EA6;&amp;#x8A2D;&amp;#x5B9A;&amp;#x70BA; 100 &amp;#x50CF;&amp;#x7D20; */
            height: 100px; /* &amp;#x9AD8;&amp;#x5EA6;&amp;#x8A2D;&amp;#x5B9A;&amp;#x70BA; 100 &amp;#x50CF;&amp;#x7D20; */
            border: 1px solid #ccc; /* &amp;#x589E;&amp;#x52A0;&amp;#x908A;&amp;#x6846;&amp;#x8B93;&amp;#x5716;&amp;#x7247;&amp;#x66F4;&amp;#x6E05;&amp;#x6670; */
            object-fit: cover; /* &amp;#x78BA;&amp;#x4FDD;&amp;#x5716;&amp;#x7247;&amp;#x586B;&amp;#x6EFF; 100x100 &amp;#x5340;&amp;#x57DF;&amp;#xFF0C;&amp;#x53EF;&amp;#x80FD;&amp;#x6703;&amp;#x88C1;&amp;#x526A;&amp;#x908A;&amp;#x7DE3;&amp;#x4F46;&amp;#x4E0D;&amp;#x6703;&amp;#x626D;&amp;#x66F2; */
            border-radius: 5px; /* &amp;#x8F15;&amp;#x5FAE;&amp;#x5713;&amp;#x89D2; */
        }

        /* --- 2. &amp;#x6A21;&amp;#x64EC; JQuiz &amp;#x6210;&amp;#x529F;&amp;#x56DE;&amp;#x994B;&amp;#x7279;&amp;#x6548; CSS --- */
